<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="SWIG"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="SWIG"
			DisplayName="SWIG"
			CommandLine="swig.exe -Wall -c++ -python -Fmicrosoft -v -o [inputs].cpp [inputs]"
			Outputs="$(InputName).cpp"
			FileExtensions="*.i"
			ExecutionDescription="SWIG is generating wrapper..."
			ShowOnlyRuleProperties="false"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
